


@model IEnumerable<DB43.Models.StudentsRegisteredInCourse>
@{

    ViewBag.Title = "RegisteredStudents";
    Layout = "~/Views/Shared/Teacherlayout.cshtml";
}

<h2>Students</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.regNo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Course_title)
        </th>
      
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.regNo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Course_title)
            </td>
      


        </tr>
    }


</table>




